{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jquery": true
    },
    "globals": {
        "FullCalendar": "readonly",
        "url": "writable",
        "page": "writable"
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "semi": true,
                "trailingComma": "all",
                "singleQuote": true,
                "printWidth": 90,
                "tabWidth": 2
            }
        ],
        "no-unused-vars": "warn",
        "no-console": "off"
    }
}